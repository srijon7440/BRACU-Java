import java.util.Scanner;
 public class ungrade01{
  public static void main(String[] args){
    Scanner sc=new Scanner (System.in);
    Systemn.out.print("N");
    int n=sc.nextInt();
    int[] arr=new int[n];
    for(int i=0;i<n;i++){
      System.out.print("Enter Element "+(i+1)+":");
      arr[i]=sc.nextInt();
    }
    
  }