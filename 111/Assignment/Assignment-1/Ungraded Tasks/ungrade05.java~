import java.util.Scanner;
 public class ungrade05{
  public static void main(String[] args){
    Scanner sc=new Scanner (System.in);
    System.out.print("M = ");
    int M=sc.nextInt();
    System.out.print("N = ");
    int N=sc.nextInt();
    int [ ] [ ] A = new int[M][N];
    
    //taking 2Darray input
    for(int i=0;i<M;i++){
      for(int j=0;j<N;j++){
        A[i][j]=sc.nextInt();
      }
    }
     //printing the  matrix A
    for(int i=0;i<M;i++){
      for(int j=0;j<N;j++){
        System.out.print(A[i][j]+"  ");
      }
      System.out.println();
    }
    //printing the  matrix Transpose of A
    for(int i=0;i<N;i++){
      for(int j=0;j<M;j++){
        System.out.print(A[i][j]+"  ");
      }
      System.out.println();
    }
  }
 }