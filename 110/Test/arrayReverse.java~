import java.util.Scanner;
public class arrayReverse{
  public static void main(String[]args){
    int[] arr={1,7,2,6,74,83};
    int x=8,temp=0;
    for(int i=0;i<6/2;i++){
      for(int j=5;j>=0;j--){
       temp=arr[i];
       arr[i]=arr[j];
       arr[j]=temp;
        }
      }
    }
  System.out.println("Reversed Array: {");
  for(int k=0;k<6;k++){
    if(k==5) System.out.println(arr[k]+"}");
    else     System.out.printl(arr[k]+",");
  }
  }
}