import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter input: ");
        String s = sc.nextLine();
        String result = "";

        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i);
            int count = 0;

            // Count how many times ch appears
            for (int j = 0; j < s.length(); j++) {
                if (s.charAt(j) == ch) {
                    count++;
                }
            }

            if (count == 1) {
                result = result + ch;  // keep if occurs once
            } else {
                // Check if this is NOT the first or last occurrence
                int first = -1;
                int last = -1;

                for (int j = 0; j < s.length(); j++) {
                    if (s.charAt(j) == ch) {
                        if (first == -1) {
                            first = j;
                        }
                        last = j;
                    }
                }

                if (i != first && i != last) {
                    result = result + ch;  // keep if not first/last
                }
            }
        }

        if (result.length() == 0) {
            System.out.println("No characters left!");
        } else {
            System.out.println(result);
        }
    }
}
