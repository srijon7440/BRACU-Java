import java.util.Scanner;
 public class Task01{
   public static void oneToN(int x,int n){
     if(n==0){
       System.out.print(n);
       return;
     }
     else{
       oneToN(x+1,n-1);
       System.out.print(n);
     }
   }
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    System.out.print("N = ");
    int n=sc.nextInt();
    oneToN(1,N);
  }
 }
    