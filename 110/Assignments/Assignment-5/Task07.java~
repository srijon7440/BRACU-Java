import java.util.Scanner;
public class Task07{
  public static void main(String[]args){
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter Row: ");
    int r=sc.nextInt();
    System.out.print("Enter Collum: ");
    int c=sc.nextInt();
    for(int i=1;i<=r;i++){
      for(int j=1;j<=c;j++){
        if(i==1||i==r){
       System.out.print(j);
        }
        else if(j==1||j==c){
          System.out.print(j);
        }
        else
          System.out.print(" ");
    }  System.out.println();   
  }
 }
}
 