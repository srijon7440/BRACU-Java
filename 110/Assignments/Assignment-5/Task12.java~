import java.util.Scanner;
 public class Task12{
  public static void main(String[] args){
   Scanner sc=new Scanner(System.in);
   System.out.print("Enter Number:");
   int n=sc.nextInt();
     for(int j=1;j<=n;j++){
       System.out.print(j);
     }
     for(int i=n-1;i>0;i--){
       System.out.print(i);
   }
  }
 }